{
    "model": "vgg11",
    "dataset": "imagenet",
    "input_info": {
      "sample_size": [1, 3, 224, 224]
    },
    "num_classes": 1000,
    "batch_size" : 64,
    "compression": {
        "algorithm": "quantization",
        "initializer": {
            "type": "min_max",
            "num_init_steps": 5
        },
        "weights":     { "mode": "symmetric", "bits": 8, "per_channel": false },
        "activations": { "mode": "symmetric", "bits": 8, "per_channel": false }
    },
    "auto_quantization":{
        "seed": 234,
        "compress_ratio": 0.15,
        "min_bit": 2,
        "max_bit": 8,
        "hidden1": 300,
        "hidden2": 300,
        "init_w": 0.003,
        "lr_c": 0.001,
        "lr_a": 0.0001,
        "rmsize": 128,
        "window_length": 1,
        "bsize": 64,
        "tau": 0.01,
		"discount": 1.0,
		"epsilon": 50000,
        "init_delta": 0.5,
        "delta_decay": 0.99,
        "warmup": 20,
        "n_update": 1,
        "train_episode": 600
    }
}